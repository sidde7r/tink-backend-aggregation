{
  "manifest_version": 2,
  "name": "Kibana Helper",
  "version": "1.0",
  "version_name": "1.0",

  "description": "This extension helps downloading HTTP debug logs from AWS",
  
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png",
    "256": "icons/icon_256.png"
  },

  "author": "Berk Gedik",

  "background": {
    "scripts": [
      "jquery_min_3.4.1.js",
      "background.js"
    ],
    "persistent": true
  },
  
  "content_scripts": [{
     "matches": [
        "https://kibana.aggregation-production.tink.network/app/kibana*", 
        "https://kibana.aggregation-staging.tink.network/app/kibana*"
      ],
     "js": ["jquery_min_3.4.1.js", "content_script.js"]
   }],

  "offline_enabled": false,

  "permissions": [
  	"webRequest",
  	"webRequestBlocking",
  	"activeTab",
  	"https://kibana.aggregation-production.tink.network/app/kibana*",
    "https://kibana.aggregation-staging.tink.network/app/kibana*"
  ]
}