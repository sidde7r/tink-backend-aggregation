msgid "{0} account"
msgid_plural "{0} accounts"
msgstr[0] "{0} compte"
msgstr[1] "{0} comptes"

#, java-format
msgid "{0} transaction"
msgid_plural "{0} transactions"
msgstr[0] "{0} transaction"
