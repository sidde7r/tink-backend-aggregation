msgid "{0} account"
msgid_plural "{0} accounts"
msgstr[0] "{0} cuenta"
msgstr[1] "{0} cuentas"

#, java-format
msgid "{0} transaction"
msgid_plural "{0} transactions"
msgstr[0] "{0} transacción"
msgstr[1] "{0} transacciones"

